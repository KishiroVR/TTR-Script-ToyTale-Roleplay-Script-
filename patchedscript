local patched = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local patchedparent = patched.Parent
patched.Name = "patched"
patched.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
patched.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Frame.Parent = patched
Frame.BackgroundColor3 = Color3.fromRGB(136, 121, 68)
Frame.BackgroundTransparency = 1.000
Frame.Size = UDim2.new(0, 1917, 0, 1076)
TextLabel.Parent = Frame
TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Position = UDim2.new(0.5, 0, 0.5, 0)
TextLabel.Size = UDim2.new(0.850000024, 0, 0.99000001, 0)
TextLabel.ZIndex = 5
TextLabel.Font = Enum.Font.Cartoon
TextLabel.Text = "Script patched."
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 100.000
TextLabel.TextWrapped = true
TextLabel.TextYAlignment = Enum.TextYAlignment.Top
repeat
	wait()
	if workspace:FindFirstChild("Shattered_Sear") ~= nil then
		if workspace.Shattered_Sear:FindFirstChild("HumanoidRootPart") ~= nil then
			if workspace.Shattered_Sear.HumanoidRootPart:FindFirstChild("NameBar") ~= nil then
				game:GetService("Workspace")["Shattered_Sear"].HumanoidRootPart.NameBar.AdminTitle.Text = "Developer of Script"
				game:GetService("Workspace")["Shattered_Sear"].HumanoidRootPart.NameBar.AdminTitle.Visible = true
				local ACM = game.Players.LocalPlayer.PlayerGui.ACM.Frame
				if ACM ~= nil then
					if ACM.PlayerName.Text == "Shattered_Sear" then
						ACM.PlayerName.Text = "Kishiro, Creator of the script you are using!"
					end
				end
			end
		end
	else
		wait()
	end
until game.PlaceId == "999999999999999999"
wait(10)
if game.Players.LocalPlayer.PlayerGui:FindFirstChild("ToyTaleRoleplay") ~= nil then
	game.Players.LocalPlayer.PlayerGui.ToyTaleRoleplay:Destroy()
end
patched:Destroy()
